<div id="<%= dom_id budget %>">
  <p>
    <strong>Start of Financial Year:</strong>
    <%= budget.start_of_financial_year %>
  </p>

  <p>
    <strong>End of Financial Year:</strong>
    <%= budget.end_of_financial_year %>
  </p>

  <p>
    <strong>Account:</strong>
    <%= budget.account.name %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= budget.amount %>
  </p>

  <p>
    <strong>Currency:</strong>
    <%= budget.currency %>
  </p>

</div>
